scalar BigDecimal

type Position {
    id: ID!
    symbol: String!
    quantity: BigDecimal!
    purchasePrice: BigDecimal!
    currentPrice: BigDecimal!
    purchaseDate: String!
}

input ClientId {
    id: String!
}

type PositionDetails {
    positions: [Position!]
    performance: BigDecimal!
}

type Query {
    positionDetails(clientId: ClientId): PositionDetails!
}